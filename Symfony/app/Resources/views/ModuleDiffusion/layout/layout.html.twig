{# Layout de la page principale #}
<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Bienvenue!{% endblock %}</title>
        {# Css Bootstrap #}
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css" integrity="sha384-1q8mTJOASx8j1Au+a5WDVnPi2lkFfwwEAa8hDDdjZlpLegxhjVME1fgjWPGmkzs7" crossorigin="anonymous">
        {# Css theme Bootstrap #}
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap-theme.min.css" integrity="sha384-fLW2N01lMqjakBkx3l/M9EahuwpSfeNvV63J5ezn3uZzapT0u7EYsXMjQV+0En5r" crossorigin="anonymous">
        {# Css jquery-ui + datepicker + style perso #}
        {% block stylesheets %}
            {% stylesheets 'css/ui-lightness/jquery-ui-1.7.2.custom.css' %}
                <link rel="stylesheet" href="{{ asset_url }}" />
            {% endstylesheets %}
            {% stylesheets 'css/jquery.datetimepicker.css' %}
                <link rel="stylesheet" href="{{ asset_url }}" />
            {% endstylesheets %}
            {% stylesheets 'css/stylesite.css' %}
                <link rel="stylesheet" href="{{ asset_url }}" />
            {% endstylesheets %}
        {% endblock %}
        {# Script Jquery #}
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.2.2/jquery.min.js"></script>
        {# Script Bootstrap #}
        <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js" integrity="sha384-0mSbJDEHialfmuBBQP6A4Qrprq5OVfW37PRR3j5ELqxss1yVqOtnepnHVP9aJ7xS" crossorigin="anonymous"></script>
    </head>
    <body>
        <div id="back"></div>
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <nav class="navbar navbar-default">
                        <ul class="nav nav-pills">
                            <li role="presentation"><a id="acce" title="Accueil" href="#">Accueil</a></li>
                            <li role="presentation"><a id="expo" title="Exposition" href="#">Agenda</a></li>
                            <li role="presentation"><a id="oeuv" title="Oeuvre" href="#">Oeuvre</a></li>
                            <li role="presentation"><a id="deco" title="Deconnexion" href="#">Info Expo</a></li>
                            <li role="presentation"><a id="deco" title="Deconnexion" href="#">Position Salle</a></li>
                        </ul>
                    </nav>
                </div>
            </div>
            <div>
                {% block body %}{% endblock %}
            </div>
        </div>
        {% block javascripts %}{% endblock %}
    </body>
</html>